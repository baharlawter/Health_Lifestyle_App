{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch API (Spring Boot)",
      "request": "launch",
      "mainClass": "com.healthylifestyle.blogapi.BlogapiApplication",
      "projectName": "blogapi"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch UI (npm start)",
      "cwd": "${workspaceFolder}/ui",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "console": "integratedTerminal"
    }
  ]
}
